{
  "name": "GitHub Blog",
  "description": "GitHub Issues Blog 订阅工具",
  "version": "0.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    "https://github.com/*",
    "storage",
    "socket"
  ],
  "icons": {
    "16": "logo16x16.jpg",
    "128": "logo128x128.jpg"
  }
}
