{
  "name": "GitHub Blog",
  "description": "GitHub Issues Blog 订阅工具",
  "version": "0.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    "https://github.com/*",
    "storage",
    "socket"
  ],
  "icons": {
    "16": "calculator-16.png",
    "128": "calculator-128.png"
  }
}