{
  "name": "GitHub Blog",
  "description": "GitHub Issues Blog 订阅工具",
  "version": "0.1",
  "manifest_version": 2,
  "content_security_policy": "font-src 'self' https://at.alicdn.com; img-src 'self' https://avatars1.githubusercontent.com",
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    "webRequest",
    "https://github.com/*",
    "https://api.github.com/*",
    "https://avatars1.githubusercontent.com/*",
    "storage"
  ],
  "icons": {
    "16": "logo16x16.png",
    "128": "logo128x128.png"
  }
}
